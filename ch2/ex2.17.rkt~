#lang sicp

;; Exercise 2.17
(define (length items)
  (define (length-iter a count)
    (if (null? a)
        count
        (length-iter (cdr a) 
                     (+ 1 count))))
  (length-iter items 0))

(define (last-pair list)
  (define (iter counter item)
    (if (= counter 1)
        item
        (iter (- counter 1) (cdr item)))))

(last-pair (list 23 72 149 34))
(34)
